{
  "path": {
    "options": [
      {
        "name": "base",
        "path": "src"
      },
      {
        "name": "action",
        "path": "src/actions"
      },
      {
        "name": "component",
        "path": "src/components"
      },
      {
        "name": "image",
        "path": "src/images"
      },
      {
        "name": "source",
        "path": "src/sources"
      },
      {
        "name": "store",
        "path": "src/stores"
      },
      {
        "name": "style",
        "path": "src/styles"
      },
      {
        "name": "test",
        "path": "test"
      },
      {
        "name": "dist",
        "path": "dist"
      }
    ]
  },
  "postcss": {
    "options": [
      {
        "name": "postcss",
        "value": "postcss",
        "packages": [
          { "name": "postcss", "version": "^5.0.21" },
          { "name": "postcss-loader", "version": "^0.9.1" }
        ]
      }
    ]
  },
  "cssmodules": {
    "options": [
      {
        "name": "cssmodules",
        "value": "cssmodules",
        "packages": [
          { "name": "react-css-modules", "version": "^3.7.10" }
        ]
      }
    ]
  },
  "style": {
    "options": [
      {
        "name": "less",
        "value": "less",
        "suffix": ".less",
        "packages": [
          { "name": "less-loader", "version": "^2.2.3" },
          { "name": "less", "version": "^2.7.1" }
        ]
      },
      {
        "name": "css",
        "value": "css",
        "suffix": ".css"
      },
      {
        "name": "sass",
        "value": "sass",
        "suffix": ".sass",
        "packages": [
          { "name": "sass-loader", "version": "^3.2.0" },
          { "name": "node-sass", "version": "^3.7.0" }
        ]
      },
      {
        "name": "scss",
        "value": "scss",
        "suffix": ".scss",
        "packages": [
          { "name": "sass-loader", "version": "^3.2.0" },
          { "name": "node-sass", "version": "^3.7.0" }
        ]
      },
      {
        "name": "stylus",
        "value": "stylus",
        "suffix": ".styl",
        "packages": [
          { "name": "stylus-loader", "version": "^2.1.0" },
          { "name": "stylus", "version": "^0.54.5" },
          { "name": "nib", "version": "~1.1.0" }
        ]
      }
    ],
    "default": "less"
  },
  "appType": {
    "options": [
      {
        "name": "single page application (单页面应用)",
        "value": "spa"
      },
      {
        "name": "multiple page application (多页面应用)",
        "value": "mpa"
      }
    ],
    "default": "spa"
  },
  "rcType": {
    "options": [
      {
        "name": "independent react component (独立的 react 组件)",
        "value": "independent"
      },
      {
        "name": "pure react (for uniform-react-components)",
        "value": "pure"
      },
      {
        "name": "wrapped react",
        "value": "wrapped"
      }
    ],
    "default": "independent"
  },
  "pageTemplate": {
    "options": [
      {
        "name": "form-table-template",
        "value": "form-table-template",
        "packages": [
          { "name": "@ali/uniform-react-components", "version": "^0.0.19"}
        ]
      }
    ]
  }
}
